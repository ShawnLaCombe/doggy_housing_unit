<p class="notice"><%= notice %></p>

<h1>Dogs</h1>

  <table class= "table table-striped table-hover" border="solid black 1px">
    <thead>
      <tr>
        <th>Id:</th>
        <th>Name:</th><br>
        <th>Breed:</th><br>
        <th>Age:</th><br>
      </tr>
    </thead>

    <tbody>
      <% @dogs.each do |dog| %>
        <tr>
          <td><%= dog.id %></td>
          <td><%= dog.name %></td>
          <td><%= dog.breed %></td>
          <td><%= dog.age %></td>
        </tr>
      <% end %>
    </tbody>
  </table>